# Admin credentials for the Ham Radio Award Coordinator
# Copy this file to .env and set your admin credentials

ADMIN_CALLSIGN=EA1RFI
ADMIN_PASSWORD=your_secure_password_here

# Domain name for HTTPS deployment (used by nginx)
DOMAIN=yourdomain.com

# MQTT / Real-time chat (set MQTT_WS_URL to enable chat)
MQTT_WS_URL=wss://yourdomain.com/mqtt
MQTT_BROKER_HOST=mosquitto
MQTT_BROKER_PORT=1883
